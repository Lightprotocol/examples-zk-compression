# Privy credentials (from https://dashboard.privy.io)
PRIVY_APP_ID=your_privy_app_id
PRIVY_APP_SECRET=your_privy_app_secret

# Treasury wallet (Privy server wallet for signing)
TREASURY_WALLET_ID=your_treasury_wallet_id
TREASURY_WALLET_ADDRESS=your_treasury_wallet_address
TREASURY_AUTHORIZATION_KEY=your_authorization_key

# Solana RPC with compression support (https://www.helius.dev)
HELIUS_RPC_URL=https://devnet.helius-rpc.com?api-key=YOUR_API_KEY

# Test token mint (optional - for testing with existing mint)
TEST_MINT=

# Default values for standalone scripts (optional)
DEFAULT_TEST_RECIPIENT=
DEFAULT_AMOUNT=0.001
DEFAULT_DECIMALS=9
